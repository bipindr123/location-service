{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "release": {
      "android": {
        "debugable": true,
        "buildType": "apk"
      }
    },
    
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "debugable": true,
        "buildType": "app-bundle"
      },
      "autoIncrement": false
    },

    "apk": {
      "android": {
        "debugable": true,
        "gradleCommand": ":app:assembleRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
